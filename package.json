{
    "name": "nuxt-forms",
    "version": "0.0.22",
    "license": "MIT",
    "type": "module",
    "workspaces": [
        "playground"
    ],
    "author": {
        "name": "Johannes Hof <johannes@disedia.com>",
        "url": "https://github.com/DevJoghurt"
    },
    "exports": {
        ".": {
          "import": "./dist/module.mjs",
          "require": "./dist/module.cjs"
        }
    },
    "typesVersions": {
        "*": {
            "types": [
                "./dist/runtime/types.d.ts"
            ]
        }
    },
    "main": "./dist/module.cjs",
    "types": "./dist/types.d.ts",
    "files": [
      "dist"
    ],
    "scripts": {
        "build": "yarn dev:prepare && nuxt-module-build",
        "dev": "nuxi dev playground",
        "dev:build": "nuxi build playground",
        "dev:prepare": "yarn nuxt-module-build --stub && nuxi prepare playground",
        "lint": "eslint --ext .js,.ts,.vue ."
    },
    "dependencies": {
        "@nuxt/kit": "^3.3.2",
        "@vee-validate/rules": "^4.8.4",
        "@vee-validate/i18n": "^4.8.3",
        "klona": "^2.0.6",
        "knitwork": "^1.0.0",
        "zod": "^3.21.4"
    },
    "devDependencies": {
        "eslint": "^8.36.0",
        "@types/node": "^18.15.11",
        "@nuxt/test-utils": "^3.3.2",
        "vitest": "^0.29.8",
        "@vitest/ui": "^0.29.8",
        "@nuxtjs/eslint-config-typescript": "^12.0.0",
        "@nuxt/module-builder": "latest",
        "@nuxt/schema": "^3.3.2"
    }
}
