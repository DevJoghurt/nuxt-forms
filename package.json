{
    "name": "nuxt-forms",
    "version": "0.0.10",
    "license": "MIT",
    "type": "module",
    "workspaces": [
        "playground"
    ],
    "author": {
        "name": "Johannes Hof <johannes@disedia.com>",
        "url": "https://github.com/DevJoghurt"
    },
    "exports": {
        ".": {
          "import": "./dist/module.mjs",
          "require": "./dist/module.cjs"
        }
    },
    "main": "./dist/module.cjs",
    "types": "./dist/types.d.ts",
    "files": [
      "dist"
    ],
    "scripts": {
        "build": "yarn dev:prepare && nuxt-module-build",
